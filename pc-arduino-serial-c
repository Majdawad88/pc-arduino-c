//git clone 
void setup() {
  Serial.begin(9600); 
  pinMode(13, OUTPUT);
}

void loop() {
  if (Serial.available() > 0) {
    char data = Serial.read();
    
    if (data == '1') {
      digitalWrite(13, HIGH);
      Serial.println("led on"); // Send message back to PC
    } 
    else if (data == '0') {
      digitalWrite(13, LOW);
      Serial.println("led off"); // Send message back to PC
    }
  }
}
